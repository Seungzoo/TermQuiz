<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.termquiz.team.mapperInterface.BoardMapper">
	
	<select id="selectList" resultType="com.termquiz.team.vo.BoardVO">
		select bNo, bId, bTitle, bTime, bCount
		from board 
		order by bNo desc
	</select>

	<select id="selectOne" resultType="com.termquiz.team.vo.QnaVO">
		select * from board where bNo=#{bNo}
	</select>
	
</mapper>